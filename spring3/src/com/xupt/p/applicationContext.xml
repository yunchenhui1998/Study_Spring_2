<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"    
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"><!-- 注意这里！！！！！！！！！！！！！！！！！！！ -->
	<bean id="person" class="com.xupt.p.Person"  p:pname="大禹" p:age="22" >
	</bean>
	
	<bean id="homeAddr" class="com.xupt.p.Address">
		<property name="addr" value="阜南"></property>
		<property name="tel" value="911"></property>
	</bean>
	
	<bean id="companyAddr" class="com.xupt.p.Address">
		<property name="addr" value="北京八宝山"></property>
		<property name="tel" value="119"></property>
	</bean>
 </beans>